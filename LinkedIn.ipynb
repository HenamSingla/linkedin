    "from selenium import webdriver\n",
    "from selenium.webdriver.common.by import By\n",
    "from selenium.webdriver.chrome.service import Service\n",
    "from selenium.webdriver.chrome.options import Options\n",
    "from webdriver_manager.chrome import ChromeDriverManager\n",
    "import time\n",
    "from datetime import datetime\n",
    "\n",
    "# LinkedIn credentials\n",
    "email = ''\n",  %add your email
    "password = ''\n",   %add your password
    "\n",
    "# URLs for universities' LinkedIn alumni pages\n",
    "urls = {\n",
    "    \"Harvard University\": \"https://www.linkedin.com/school/harvard-university/people/?keywords=medical\",\n",
    "    \"Johns Hopkins University\": \"https://www.linkedin.com/school/johns-hopkins-university/people/?keywords=medical\",\n",
    "    \"University of Pennsylvania (Perelman)\": \"https://www.linkedin.com/school/university-of-pennsylvania/people/?keywords=medical\",\n",
    "    \"Columbia University\": \"https://www.linkedin.com/school/columbia-university/people/?keywords=medical\",\n",
    "    \"Duke University\": \"https://www.linkedin.com/school/duke-university/people/?keywords=medical\",\n",
    "    \"Stanford University\": \"https://www.linkedin.com/school/stanford-university/people/?keywords=medical\",\n",
    "    \"University of California--San Francisco\": \"https://www.linkedin.com/school/ucsf/people/?keywords=medical\",\n",
    "    \"Vanderbilt University\": \"https://www.linkedin.com/school/vanderbilt-university/people/?keywords=medical\",\n",
    "    \"Washington University in St. Louis\": \"https://www.linkedin.com/school/washington-university-in-st-louis/people/?keywords=medical\",\n",
    "    \"Cornell University (Weill)\": \"https://www.linkedin.com/school/cornell-university/people/?keywords=medical\",\n",
    "    \"New York University (Grossman)\": \"https://www.linkedin.com/school/new-york-university/people/?keywords=medical\",\n",
    "    \"Yale University\": \"https://www.linkedin.com/school/yale-university/people/?keywords=medical\",\n",
    "    \"Mayo Clinic School of Medicine (Alix)\": \"https://www.linkedin.com/school/mayo-clinic-alix-school-of-medicine/people/?keywords=medical\",\n",
    "    \"Northwestern University (Feinberg)\": \"https://www.linkedin.com/school/northwestern-university/people/?keywords=medical\",\n",
    "    \"University of Michigan--Ann Arbor\": \"https://www.linkedin.com/school/university-of-michigan/people/?keywords=medical\",\n",
    "    \"University of Pittsburgh\": \"https://www.linkedin.com/school/university-of-pittsburgh/people/?keywords=medical\",\n",
    "    \"University of Washington\": \"https://www.linkedin.com/school/university-of-washington/people/?keywords=medical\",\n",
    "    \"Icahn School of Medicine at Mount Sinai\": \"https://www.linkedin.com/school/icahnmountsinai/people/?keywords=medical\",\n",
    "    \"University of California--Los Angeles (Geffen)\": \"https://www.linkedin.com/school/ucla/people/?keywords=medical\",\n",
    "    \"University of Chicago (Pritzker)\": \"https://www.linkedin.com/school/uchicago/people/?keywords=medical\"\n",
    "}\n",
    "\n",
    "# Setup the Chrome WebDriver\n",
    "options = Options()\n",
    "options.headless = False  # Run in headless mode if needed\n",
    "driver = webdriver.Chrome(service=Service(ChromeDriverManager().install()), options=options)\n",
    "\n",
    "def linkedin_login(email, password):\n",
    "    driver.get(\"https://www.linkedin.com/login\")\n",
    "    time.sleep(2)  # Wait for the page to load\n",
    "\n",
    "    # Find and fill the email field\n",
    "    email_field = driver.find_element(By.ID, \"username\")\n",
    "    email_field.send_keys(email)\n",
    "    \n",
    "    # Find and fill the password field\n",
    "    password_field = driver.find_element(By.ID, \"password\")\n",
    "    password_field.send_keys(password)\n",
    "    \n",
    "    # Submit the login form\n",
    "    sign_in_button = driver.find_element(By.XPATH, \"//button[@type='submit']\")\n",
    "    sign_in_button.click()\n",
    "    \n",
    "    time.sleep(5)  # Wait for the login to complete\n",
    "\n",
    "def get_medical_alumni_count(url):\n",
    "    try:\n",
    "        driver.get(url)\n",
    "        time.sleep(5)  # Wait for the page to load\n",
    "\n",
    "        # Check if the user is logged in\n",
    "        if \"login\" in driver.current_url:\n",
    "            print(\"Login failed.\")\n",
    "            return None\n",
    "\n",
    "        # Attempt to find the alumni count element\n",
    "        try:\n",
    "            count_element = driver.find_element(By.CSS_SELECTOR, 'h2.text-heading-xlarge')\n",
    "        except Exception as e:\n",
    "            print(f\"Error locating the alumni count element: {e}\")\n",
    "            return None\n",
    "\n",
    "        count_text = count_element.text.strip()\n",
    "        count = ''.join(filter(str.isdigit, count_text))\n",
    "        return int(count)\n",
    "    except Exception as e:\n",
    "        print(f\"Error fetching data from {url}: {e}\")\n",
    "        return None\n",
    "\n",
    "def print_bold(text):\n",
    "    bold_text = f\"\\033[1m{text}\\033[0m\"\n",
    "    print(bold_text)\n",
    "\n",
    "# Perform LinkedIn login\n",
    "linkedin_login(email, password)\n",
    "\n",
    "# Get the alumni count for each university\n",
    "alumni_counts = {}\n",
    "total_count = 0\n",
    "for university, url in urls.items():\n",
    "    count = get_medical_alumni_count(url)\n",
    "    if count:\n",
    "        alumni_counts[university] = count\n",
    "        total_count += count\n",
    "    else:\n",
    "        alumni_counts[university] = 'N/A'\n",
    "\n",
    "# Close the WebDriver\n",
    "driver.quit()\n",
    "\n",
    "# Print the results with timestamp\n",
    "timestamp = datetime.now().strftime(\"%Y-%m-%d %H:%M:%S\")\n",
    "print_bold(f\"Data extracted on: {timestamp}\")\n",
    "\n",
    "for university, count in alumni_counts.items():\n",
    "    print(f\"{university} Medical Alumni Count: {count}\")\n",
    "    \n",
    "print_bold(f\"Total Medical Alumni Count: {total_count}\")"
   ]
